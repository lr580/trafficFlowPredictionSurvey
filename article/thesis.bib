@inproceedings{xie_sast-gnn_2020,
	address = {Cham},
	title = {{SAST}-{GNN}: {A} {Self}-{Attention} {Based} {Spatio}-{Temporal} {Graph} {Neural} {Network} for {Traffic} {Prediction}},
