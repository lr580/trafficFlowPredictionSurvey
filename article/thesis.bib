@inproceedings{dai_hybrid_2020,
	address = {New York, NY, USA},
	series = {{KDD} '20},
	title = {Hybrid {Spatio}-{Temporal} {Graph} {Convolutional} {Network}: {Improving} {Traffic} {Prediction} with {Navigation} {Data}},
