	journal = {IEEE Transactions on Pattern Analysis and Machine Intelligence},
	author = {Wang, Yunbo and Wu, Haixu and Zhang, Jianjin and Gao, Zhifeng and Wang, Jianmin and Yu, Philip S. and Long, Mingsheng},
