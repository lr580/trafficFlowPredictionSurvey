	note = {Conference Name: IEEE Transactions on Intelligent Transportation Systems},
	keywords = {Data models, Predictive models, Feature extraction, Forecasting, Correlation, Traffic prediction, neural networks, Solid modeling, 3D convolutions, recalibration block, spatial–temporal data, Three-dimensional displays},
	pages = {3913--3926},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\lr580\\Zotero\\storage\\LS5HYWGV\\8684259.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\lr580\\Zotero\\storage\\B9T6HWP7\\Guo 等 - 2019 - Deep Spatial–Temporal 3D Convolutional Neural Netw.pdf:application/pdf},
