	note = {ISSN: 2161-4407},
	keywords = {deep learning, Machine learning, Neural networks, Predictive models, Roads, traffic prediction, attention mechinism, Convolution, Feature extraction, Forecasting, spatial-temporal},
	pages = {1--8},
	file = {已提交版本:C\:\\Users\\lr580\\Zotero\\storage\\JT5QIVLA\\Cheng 等 - 2018 - DeepTransport Learning Spatial-Temporal Dependenc.pdf:application/pdf;IEEE Xplore Abstract Record:C\:\\Users\\lr580\\Zotero\\storage\\7P5ACD72\\8489600.html:text/html},
