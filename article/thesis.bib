	note = {Conference Name: IEEE Transactions on Knowledge and Data Engineering},
	keywords = {Deep learning, Data models, Predictive models, Urban areas, Spatiotemporal phenomena, spatiotemporal data mining, Gravity, Field theory for human mobility, interpretable traffic prediction, Potential energy, potential energy fields},
