	doi = {10.24963/ijcai.2022/309},
	file = {Full Text PDF:C\:\\Users\\lr580\\Zotero\\storage\\QQV9YGUK\\Shao 等 - 2022 - Long-term Spatio-temporal Forecasting via Dynamic .pdf:application/pdf},
