	month = jul,
	year = {2017},
	note = {Number: 7
Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {convolutional neural network, long short-term memory, network representation, spatiotemporal feature, traffic prediction},
	pages = {1501},
	file = {Full Text PDF:C\:\\Users\\lr580\\Zotero\\storage\\FBGEWJJS\\Yu 等 - 2017 - Spatiotemporal Recurrent Convolutional Networks fo.pdf:application/pdf},
