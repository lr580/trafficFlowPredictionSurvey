	author = {Kwak, Semin and Geroliminis, Nikolas and Frossard, Pascal},
	year = {2021},
	note = {Conference Name: IEEE Transactions on Signal and Information Processing over Networks},
	keywords = {Predictive models, Correlation, Time series analysis, Bayes methods, Bayesian inference, dynamic linear model, heat diffusion model, Multivariate time series forecasting},
	pages = {648--659},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\lr580\\Zotero\\storage\\3WKFZS5K\\9565380.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\lr580\\Zotero\\storage\\VMYNCM65\\Kwak 等 - 2021 - Traffic Signal Prediction on Transportation Networ.pdf:application/pdf},
}

