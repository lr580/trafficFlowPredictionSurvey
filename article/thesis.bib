}

@inproceedings{zhuang_uncertainty_2022,
	address = {New York, NY, USA},
	series = {{KDD} '22},
	title = {Uncertainty {Quantification} of {Sparse} {Travel} {Demand} {Prediction} with {Spatial}-{Temporal} {Graph} {Neural} {Networks}},
