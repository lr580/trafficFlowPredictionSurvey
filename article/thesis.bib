	pages = {4795--4806},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\lr580\\Zotero\\storage\\JT572EAR\\8580534.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\lr580\\Zotero\\storage\\5XVM8LET\\He 等 - 2019 - STANN A Spatio–Temporal Attentive Neural Network .pdf:application/pdf},
}

