@inproceedings{sun_predicting_2021,
	title = {Predicting traffic congestion evolution : a deep meta learning approach},
	copyright = {© 2021 International Joint Conferences on Artificial Intelligence. All rights reserved.  This paper was published in Proceedings of the Thirtieth International Joint Conference on Artificial Intelligence (IJCAI-21) and is made available with permission of International Joint Conferences on Artificial Intelligence.},
