	note = {Conference Name: IEEE Transactions on Knowledge and Data Engineering},
	keywords = {Traffic forecasting, Roads, Forecasting, Correlation, Decoding, graph attention network, Heuristic algorithms, dynamic graph, multi-hop, Spread spectrum communication, Transformers},
