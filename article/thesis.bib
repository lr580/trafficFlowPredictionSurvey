	note = {Conference Name: IEEE Transactions on Knowledge and Data Engineering},
	keywords = {Deep learning, Graph neural networks, Task analysis, Transportation, Time series analysis, Topology, Graph Neural Networks, Predictive Learning, Spatio-Temporal Data Mining, Surveys, Time Series, Urban Computing},
	pages = {1--20},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\lr580\\Zotero\\storage\\APBH8TNW\\10328393.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\lr580\\Zotero\\storage\\KX2HQXE7\\Jin 等 - 2023 - Spatio-Temporal Graph Neural Networks for Predicti.pdf:application/pdf},
