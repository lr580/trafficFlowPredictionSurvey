	note = {ISSN: 2576-8565},
	keywords = {Predictive models, Feature extraction, Real-time systems, Urban areas, Vehicular ad hoc networks, Quality of service, Vehicle-to-infrastructure},
	pages = {1--6},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\lr580\\Zotero\\storage\\R52XYXQW\\9919973.html:text/html},
