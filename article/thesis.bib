	keywords = {Deep learning, Graph neural networks, Data models, Predictive models, Feature extraction, Forecasting, Traffic control, Intelligent transportation systems, Computer architecture, traffic flow forecasting, Spatiotemporal phenomena, graph neural network, Surveys, Long short term memory, spatial-temporal dependence},
