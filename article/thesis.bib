@inproceedings{zhang_cmt-net_2022,
	address = {New York, NY, USA},
	series = {{WSDM} '22},
	title = {{CMT}-{Net}: {A} {Mutual} {Transition} {Aware} {Framework} for {Taxicab} {Pick}-ups and {Drop}-offs {Co}-{Prediction}},
