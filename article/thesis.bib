	note = {ISSN: 2161-4407},
	keywords = {Deep learning, Data models, Predictive models, Roads, Forecasting, Generators},
	pages = {1--8},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\lr580\\Zotero\\storage\\J5RLCSDJ\\8852211.html:text/html},
}

