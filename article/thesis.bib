@article{cui_traffic_2020,
	title = {Traffic {Graph} {Convolutional} {Recurrent} {Neural} {Network}: {A} {Deep} {Learning} {Framework} for {Network}-{Scale} {Traffic} {Learning} and {Forecasting}},
