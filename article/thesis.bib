	month = mar,
	year = {1998},
	note = {Publisher: Royal Society},
	keywords = {empirical mode decomposition, frequency-time spectrum, Hilbert spectral analysis, intrinsic time scale, non-stationary time series, nonlinear differential equations},
	pages = {903--995},
