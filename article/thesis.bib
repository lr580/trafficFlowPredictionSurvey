	file = {IEEE Xplore Abstract Record:C\:\\Users\\lr580\\Zotero\\storage\\GIE45QDU\\10400973.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\lr580\\Zotero\\storage\\G52V84JZ\\Shin 和 Yoon - 2024 - PGCN Progressive Graph Convolutional Networks for.pdf:application/pdf},
